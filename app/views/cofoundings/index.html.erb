<div class="page-header">
  <h1>
    Cofoundings
    <a href="/cofoundings/new" class="btn btn-lg btn-success">New Cofounding</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Founder</th>
        <th>Startup</th>
        <th>Actions</th>
      </tr>

      <% @cofoundings.each do |cofounding| %>
      <tr>
        <td><%= cofounding.founder_id %></td>
        <td><%= cofounding.startup_id %></td>
        <td>
          <a href="/cofoundings/<%= cofounding.id %>" class="btn btn-primary">Show</a>
          <a href="/cofoundings/<%= cofounding.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_cofounding/<%= cofounding.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
