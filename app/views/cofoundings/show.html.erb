<div class="page-header">
  <h3>Cofounding #<%= @cofounding.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Founder</dt>
      <dd><%= @cofounding.founder_id %></dd>

      <dt>Startup</dt>
      <dd><%= @cofounding.startup_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/cofoundings" class="btn btn-primary">
        Back
      </a>
      <a href="/cofoundings/<%= @cofounding.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_cofounding/<%= @cofounding.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
